{
  "name": "backend-interactive-video-client",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "nodemon server.js",
    "dev": "nodemon server.js",

    "test": "NODE_ENV=test jest",
    "test:unit": "NODE_ENV=test jest --testPathPattern=unit",
    "test:integration": "NODE_ENV=test jest --testPathPattern=integration",
    "test:api": "NODE_ENV=test jest --testPathPattern=api",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:ci": "NODE_ENV=test jest --coverage --watchAll=false --ci",
    "test:performance": "NODE_ENV=test k6 run tests/performance/*.js",
    "test:e2e": "NODE_ENV=test cypress run"

  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.693.0",
    "@azure/msal-node": "^2.14.0",
    "@google-cloud/storage": "^7.16.0",
    "@google/genai": "^1.3.0",
    "@google/generative-ai": "^0.24.0",
    "@gradio/client": "^1.13.0",
    "@huggingface/inference": "^3.4.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "assemblyai": "^4.9.0",
    "async": "^3.2.6",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "chai": "^5.2.0",
    "chai-http": "^5.1.2",
    "cloudinary": "^2.5.1",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dateformat": "^5.0.3",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^4.21.1",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.18.0",
    "form-data": "^4.0.1",
    "google-auth-library": "^9.14.1",
    "gridfs-stream": "1.1.1",
    "ioredis": "^5.4.1",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "kafka-node": "^5.0.0",
    "minio": "^8.0.2",
    "mocha": "^11.5.0",
    "monacopilot": "^1.1.18",
    "mongodb": "^6.10.0",
    "mongoose": "^8.6.3",
    "msal-node": "^0.0.1-security",
    "multer": "^1.4.5-lts.1",
    "multer-gridfs-storage": "5.0.2",
