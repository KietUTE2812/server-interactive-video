    "nodemailer": "^7.0.3",
    "nodemon": "^3.1.7",
    "openai": "^4.72.0",
    "passport": "^0.7.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "querystring": "^0.2.1",
    "request": "^2.88.2",
    "socket.io": "^4.8.1",
    "uuid": "^10.0.0",

    "vnpay": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^29.7.0",
    "cypress": "^13.6.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.0",
    "supertest": "^6.3.3",
    "k6": "^0.47.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup/setupTests.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/tests/**/*.spec.js"
    ],
    "collectCoverageFrom": [
      "controllers/**/*.js",
      "models/**/*.js",
      "services/**/*.js",
      "utils/**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/tests/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }

  }
}